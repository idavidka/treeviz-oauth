{
	"name": "@treeviz/oauth",
	"version": "1.0.0",
	"description": "TreeViz OAuth 2.0 client for federated authentication",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "tsc --noEmit",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm run build",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [
		"treeviz",
		"oauth",
		"oauth2",
		"authentication",
		"federated-auth"
	],
	"author": "idavidka and @treeviz contributors",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git@github.com:idavidka/treeviz-oauth.git"
	},
	"homepage": "https://github.com/idavidka/treeviz-oauth#readme",
	"bugs": {
		"url": "https://github.com/idavidka/treeviz-oauth/issues"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"tsup": "^8.0.0",
		"typescript": "^5.3.0",
		"vitest": "^1.0.0"
	}
}
